{
	"name": "milkyskies",
	"version": "0.0.1",
	"private": true,
	"license": "MIT",
	"scripts": {
		"dev": "yarn prepare && vite dev",
		"prepare": "yarn generate",
		"build": "yarn prepare && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"generate": "npm run generate:sveltekit && npm run generate:api && npm run generate:prisma",
		"generate:sveltekit": "svelte-kit sync",
		"generate:api": "svetch.ts",
		"generate:prisma": "prisma generate"
	},
	"devDependencies": {
		"@catppuccin/tailwindcss": "^0.1.6",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.27.4",
		"@types/spotify-web-api-node": "^5.0.11",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.30.0",
		"postcss": "^8.4.32",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.0.0",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"prisma": "^5.7.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"tsx": "^4.6.2",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/space-mono": "^5.0.18",
		"@japanese-monospaced-fonts/noto-sans-mono-cjk-jp": "^1.0.1",
		"@lucia-auth/adapter-prisma": "^3.0.2",
		"@prisma/client": "^5.7.0",
		"axios": "^1.6.2",
		"lucia": "^2.7.4",
		"spotify-web-api-node": "^5.0.2",
		"svetch.ts": "^1.0.47",
		"zod": "^3.22.4"
	}
}
